{
  "name": "hr-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:back": "nodemon --watch server --exec npx babel-node -- ./server",
    "start:front": "ng serve --proxy-config proxy.conf.json",
    "build:back": "babel ./server -d ./dist",
    "build:front": "ng build --dev",
    "build:front:prod": "ng build --prod",
    "build:stats:front": "ng build --dev --stats-json=stats.json",
    "build:stats:front:prod": "ng build --prod --stats-json=stats.json",
    "build:all": "rm -rf dist && npm run \"build:back\" && npm run \"build:front\"",
    "build:all:prod": "rm -rf dist && npm run \"build:back\" && npm run \"build:front:prod\"",
    "lint:back": "eslint server",
    "lint:front": "ng lint",
    "test:front": "ng test",
    "e2e:front": "ng e2e",
    "stats:front": "webpack-bundle-analyzer ./dist/public/stats.json",
    "stylelint": "stylelint '**/*.scss'"
  },
  "private": true,
  "dependencies": {
    "@godaddy/terminus": "2.2.0",
    "cors": "2.8.4",
    "dotenv": "5.0.1",
    "express": "4.16.3",
